    -unity-background-scale-mode: scale-to-fit;
    flex-shrink: 0;
    flex-grow: 0;
    padding-top: 0;
    padding-right: 0;
    padding-bottom: 14px;
    padding-left: 0;
    width: 302px;
    height: 141px;
    -unity-font-definition: url("project://database/Assets/GemHunterMatch/Fonts/CurseCasual/Curse%20Casual%20SDF.asset?fileID=11400000&guid=0a460f6750bafb142b0340a5e205adf4&type=2#Curse Casual SDF");
