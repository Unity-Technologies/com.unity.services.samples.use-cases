.link-account-container {
    flex-grow: 1;
    flex-direction: row;
    align-items: stretch;
    justify-content: space-around;
    margin-top: 0;
    margin-right: 0;
    margin-bottom: 0;
    margin-left: 0;
    align-content: stretch;
    padding-top: 0;
    padding-right: 0;
    padding-bottom: 0;
    padding-left: 0;
    flex-shrink: 1;
    align-self: stretch;
    width: auto;
    height: 30px;
    -unity-slice-scale: 1px;
    -unity-slice-left: 0;
    -unity-slice-top: 0;
    -unity-slice-right: 0;
    -unity-slice-bottom: 0;
    -unity-background-scale-mode: stretch-to-fill;
}

.account-connected-icon {
    flex-grow: 0;
    background-image: url("project://database/Assets/GemHunterUGS/Art/UI_Assets-Updated.psd?fileID=-6053796608419976351&guid=ac8d91374d183b142b4426b3604fdc7d&type=3#ic_checkbox_bg_disable");
    width: 120px;
    height: 120px;
    flex-shrink: 0;
    justify-content: center;
    align-self: center;
    align-content: center;
    align-items: center;
    margin: 20px;
    -unity-background-image-tint-color: rgb(255, 255, 255);
    position: relative;
    flex-direction: column;
    left: auto;
    bottom: auto;
    right: auto;
    -unity-background-scale-mode: scale-to-fit;
    display: flex;
    margin-top: 0;
    margin-bottom: 0;
    margin-left: 0;
}

.unlink-account-button {
    background-image: url("project://database/Assets/GemHunterUGS/Art/UI_Assets-Updated.psd?fileID=5158897372892210589&guid=ac8d91374d183b142b4426b3604fdc7d&type=3#btn_delete");
    width: 120px;
    height: 120px;
    align-self: center;
    color: rgba(230, 230, 230, 0);
    background-color: rgba(188, 188, 188, 0);
    border-left-color: rgba(149, 149, 149, 0);
    border-right-color: rgba(149, 149, 149, 0);
    border-top-color: rgba(149, 149, 149, 0);
    border-bottom-color: rgba(149, 149, 149, 0);
    border-top-width: 0;
    border-right-width: 0;
    border-bottom-width: 0;
    border-left-width: 0;
    -unity-background-image-tint-color: rgb(255, 255, 255);
    margin: 20px;
    align-items: center;
    align-content: center;
    position: relative;
    right: auto;
    left: auto;
    flex-shrink: 0;
    flex-grow: 0;
    -unity-background-scale-mode: scale-to-fit;
    margin-top: 0;
    margin-right: 0;
    margin-bottom: 0;
    margin-left: 20px;
    justify-content: center;
    display: flex;
}

.account-buttons {
